#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x91b8db8 .scope module, "testbench" "testbench" 2 6;
 .timescale 0 0;
v0x91e4bf0_0 .net "clk", 0 0, v0x91e49e0_0;  1 drivers
v0x91e4c90_0 .net "data", 7 0, v0x91e4a58_0;  1 drivers
v0x91e4d30_0 .net "enb", 0 0, v0x91e4ad0_0;  1 drivers
v0x91e4dc0_0 .net "out", 0 0, v0x91e46f8_0;  1 drivers
S_0x91b9cb0 .scope module, "ser1" "serializador" 2 15, 3 5 0, S_0x91b8db8;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "enb"
    .port_info 1 /INPUT 8 "data"
    .port_info 2 /INPUT 1 "clk"
    .port_info 3 /OUTPUT 1 "out"
v0x91b9dd8_0 .net "clk", 0 0, v0x91e49e0_0;  alias, 1 drivers
v0x91e4598_0 .var "counter", 3 0;
v0x91e4610_0 .net "data", 7 0, v0x91e4a58_0;  alias, 1 drivers
v0x91e4690_0 .net "enb", 0 0, v0x91e4ad0_0;  alias, 1 drivers
v0x91e46f8_0 .var "out", 0 0;
v0x91e4788_0 .var "rst", 0 0;
v0x91e47f0_0 .var "temp", 7 0;
E_0x91b9db0/0 .event negedge, v0x91e4788_0;
E_0x91b9db0/1 .event posedge, v0x91b9dd8_0;
E_0x91b9db0 .event/or E_0x91b9db0/0, E_0x91b9db0/1;
S_0x91e48a8 .scope module, "test1" "tester" 2 14, 4 6 0, S_0x91b8db8;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "enb"
    .port_info 1 /OUTPUT 8 "data"
    .port_info 2 /OUTPUT 1 "clk"
    .port_info 3 /INPUT 1 "out"
v0x91e49e0_0 .var "clk", 0 0;
v0x91e4a58_0 .var "data", 7 0;
v0x91e4ad0_0 .var "enb", 0 0;
v0x91e4b60_0 .net "out", 0 0, v0x91e46f8_0;  alias, 1 drivers
    .scope S_0x91e48a8;
T_0 ;
    %delay 2, 0;
    %load/vec4 v0x91e49e0_0;
    %nor/r;
    %store/vec4 v0x91e49e0_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x91e48a8;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91e49e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91e4ad0_0, 0, 1;
    %vpi_call 4 20 "$display", "Inicia Prueba, Cargamos 10001000" {0 0 0};
    %pushi/vec4 136, 0, 8;
    %store/vec4 v0x91e4a58_0, 0, 8;
    %delay 50, 0;
    %vpi_call 4 23 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x91b9cb0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x91e4788_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x91b9cb0;
T_3 ;
    %wait E_0x91b9db0;
    %load/vec4 v0x91e4788_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_3.0, 4;
    %load/vec4 v0x91e4610_0;
    %assign/vec4 v0x91e47f0_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x91e4598_0, 0;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x91e47f0_0;
    %pushi/vec4 7, 0, 32;
    %load/vec4 v0x91e4598_0;
    %pad/u 32;
    %sub;
    %part/u 1;
    %assign/vec4 v0x91e46f8_0, 0;
    %load/vec4 v0x91e4598_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x91e4598_0, 0;
T_3.1 ;
    %load/vec4 v0x91e4598_0;
    %cmpi/e 7, 0, 4;
    %jmp/0xz  T_3.2, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x91e4788_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x91e4598_0, 0;
    %jmp T_3.3;
T_3.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x91e4788_0, 0, 1;
T_3.3 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x91b8db8;
T_4 ;
    %vpi_call 2 20 "$dumpfile", "registro.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars" {0 0 0};
    %delay 50, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "Serializador_tb.v";
    "SerializadorUv.v";
    "tester1.v";
